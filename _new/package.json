{
  "name": "davidcalhounphotography.com ",
  "version": "1.0.0",
  "description": "Personal photography website of David Calhoun",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r dist/* || true",
    "build-dev": "npm run clean && webpack -d --mode development && npm run cp-static",
    "build-prod": "npm run clean && webpack -p --mode production && npm run cp-static",
    "build": "npm run build-prod",
    "cp-static": "cp -r src/static/* dist",
    "start": "npm run cp-static && webpack-dev-server --hot --mode development",
    "dev": "npm start",
    "test": "jest",
    "bump": "! npm outdated && npm update && npm audit fix || echo 'Package up to date'"
  },
  "author": "David Calhoun",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-syntax-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@hot-loader/react-dom": "^16.13.0",
    "@testing-library/dom": "^7.7.0",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.4",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "html-webpack-template": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "postcss-custom-media": "^7.0.8",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.1",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "mini-css-extract-plugin": "^0.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-hover-slideshow": "^2.1.8",
    "react-markdown": "^4.3.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
